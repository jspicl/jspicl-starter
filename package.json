{
  "name": "jspicl-starter",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/jspicl/jspicl-starter"
  },
  "type": "module",
  "description": "A starter template for jspicl projects",
  "scripts": {
    "restore-cart": "cross-env copyfiles game.p8 build",
    "prebuild": "yarn restore-cart -s",
    "build": "jspicl src/index.ts build/game.p8 -c ./jspicl.config.ts",
    "start": "yarn prebuild && yarn build --watch"
  },
  "author": "Agron Kabashi",
  "license": "MIT",
  "devDependencies": {
    "@jspicl/cli": "^4.0.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0"
  },
  "packageManager": "yarn@4.12.0"
}
